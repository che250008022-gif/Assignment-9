#include<iostream>
using namespace std;
struct Item{
    int id;
    float cost;
};
int main() {
    Item structures[6];
    structures[0]={0,0.1};
    structures[1]={1,50.01};
    structures[2]={2,2.2};
    structures[3]={3,3.3};
    structures[4]={4,53.3};
    structures[5]={10,100};
    for(int i=0 ; i<6 ; i++){
        if(structures[i].cost>50.00){
            cout<<structures[i].id<<endl<<structures[i].cost<<endl;
        }
    }
    return 0;
}

